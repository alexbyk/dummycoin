{
  "defaultSeverity": "error",
  "extends": [
    "tslint:recommended"
  ],
  "jsRules": {},
  "rules": {
    "ordered-imports": false,
    "member-access": false,
    "object-literal-sort-keys": false,
    "quotemark": [
      true,
      "single",
      "avoid-escape"
    ],
    "curly": [
      true,
      "ignore-same-line"
    ],
    "arrow-parens": false,
    "arrow-return-shorthand": true,
    "member-ordering": false,
    "max-classes-per-file": false
  }
}
